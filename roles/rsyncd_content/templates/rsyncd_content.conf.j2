gid = users
read only = true
use chroot = true
transfer logging = true
log format = %h %o %f %l %b
log file = /var/log/rsyncd.log
pid file = /var/run/rsyncd.pid

[content]
    path = {{ webapp.content_path }}
    comment = FirstSpirit webapp content
    read only = false
    uid = webapp

[datasources]
    path = {{ webapp.datasources_path }}
    comment = FirstSpirit datasources files
    read only = false
    uid = webapp
